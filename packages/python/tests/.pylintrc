[MESSAGES CONTROL]
init-hook="from pylint.config import find_pylintrc; import os, sys; sys.path.append(f'{os.path.dirname(find_pylintrc())}/packages/python')"

disable=
	missing-class-docstring,
	missing-function-docstring,
	missing-module-docstring,
	import-error,

good-names=_,
  i, j, k,
  x, y, z,
  ex, pr, fp
